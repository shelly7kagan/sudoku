{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file",
            "command": "/usr/bin/g++-12",
            "args": [
                "-std=c++17",
                "-fdiagnostics-color=always",
                "-g",
                "sudoku_board.cc",
                "sudoku_solver.cc",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "compiler: /usr/bin/g++-12"
        }
    ],
    "version": "2.0.0"
}